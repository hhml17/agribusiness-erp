{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "content-type": "text/javascript; charset=utf-8"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.js", "/*.css", "/*.json", "/*.ico", "/*.png", "/*.jpg", "/*.svg"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".json": "application/json",
    ".css": "text/css",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".ico": "image/x-icon"
  },
  "globalHeaders": {
    "cache-control": "no-cache"
  }
}
